HW1
      package com.company;
      import static com.company.ibio.Helpers.*;
      import ibadts.IBCollection;

      public class Main
      {
          public static void main(String[] args)
          {
              IBCollection<String> names = new IBCollection<>();
              names.add (new String[] {"John", "Marie"});

              for (int count = 0; count <= 2; count++)
              {
                  names.resetNext();
                  while(names.hasNext())
                      output(names.getNext());
              }
          }
      }
      
      
HW2

      package com.company;
      import static com.company.ibio.Helpers.*;
      import ibadts.IBCollection;
      import java.util.Scanner;

      public class Main
      {
          public static void main(String[] args)
          {
              IBCollection<Integer> numbers = new IBCollection<>();
              Scanner scan = new Scanner(System.in);
              int t = 0;
              boolean flag = true;
              int number = 0;

              while (flag)
              {
                  number = inputInt();

                  if (number <= 0)
                      flag = false;

                  else
                  {
                      numbers.addItem(number);
                      t = number;
                  }
              }

              while (numbers.hasNext())
              {
                  number = numbers.getNext();

                  if (number > t)
                      output(number);
              }
          }
      }
