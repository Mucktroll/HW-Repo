package com.company;
import static com.company.ibio.Helpers.*;

public class Main
{
    public static void main(String[] args)
    {
        int[] numbers = {-4, 11, 7, -12, 6, 1};

        for (int i = 0; i < numbers.length - 1; i++)
        {
            int smallestInd = i;

            for (int c = i + 1; c < numbers.length; c++)
            {
                if (numbers[smallestInd] > numbers[c])
                {
                    smallestInd = c;
                    for (int n : numbers)
                        System.out.print(n + " ");
                    output(" ");

                }
            }

            swap(numbers, i, smallestInd);
        }

        for (int n : numbers)
            System.out.print(n + " ");

    }

    static void swap (int[] array,int ind1, int ind2)
    {
        int temp = array[ind1];
        array[ind1] = array[ind2];
        array[ind2] = temp;
    }

}
