package com.company;
import static com.company.ibio.Helpers.*;

public class Main
{
    public static void main(String[] args)
    {
        Newspaper [] news = new Newspaper[1];
        news[0] = new Newspaper("The Daily Mail", 2000, 4, 11, true);

        for (Newspaper n : news)
            System.out.println(n);
    }
}

package com.company;

public class Newspaper
{
    public Newspaper (String name, int year, int month, int dayOfMonth, boolean isFinnish)
    {
        this.name = name;
        this.year = year;
        this.month = month;
        this.dayOfMonth = dayOfMonth;
        this.isFinnish = isFinnish;
    }

    public String getName()
    {
        return name;
    }

    public void setDate (int year, int month, int dayOfMonth)
    {

    }

    public int getYear()
    {
        return year;
    }

    public int getMonth()
    {
        return month;
    }

    public int getDayOfMonth()
    {
        return dayOfMonth;
    }

    public boolean getIsFinnish()
    {
        return isFinnish;
    }

    public String toString()
    {
        return "The Newspaper called " + name + " was published on " + dayOfMonth + "." + month + "." + year + ". Is it Finnish? " + isFinnish;
    }

    private String name;
    private int year;
    private int month;
    private int dayOfMonth;
    private boolean isFinnish;
}
